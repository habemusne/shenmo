$primaryColorHue1Transparent2: rgba(0, 0, 0, 0.9);

i.icon{
  display: inline-block;
  vertical-align: middle;
  background-size: 100% auto;
  background-position: center;
  background-repeat: no-repeat;
  font-style: normal;
  position: relative;
}


$primaryColorHue1: rgb(0, 150, 136);
$primaryColorHue1Transparent1: rgba(0, 0, 0, 0.2);
$primaryColorHue1Transparent2: rgba(0, 0, 0, 0.9);

i.icon{
  display: inline-block;
  vertical-align: middle;
  background-size: 100% auto;
  background-position: center;
  background-repeat: no-repeat;
  font-style: normal;
  position: relative;
}

i.icon.icon-good{
  width: 100px;
  height: 100px;
  background-image: url("<%= 'good.png' %>")
}

i.icon.icon-infinity{
  width: 100px;
  height: 100px;
  background-image: url("<%= 'infinity.png' %>")
}

i.icon.icon-light{
  width: 100px;
  height: 100px;
  background-image: url("<%= 'light.png' %>")
}
